<img class="garuda-header" src="/assets/garuda-header.png">

<hr>
<span>
<% if user_signed_in? %>
  <ul><%= link_to "Logout", destroy_user_session_path, method: :delete %></ul>
<% else %>
  <ul><%= link_to "sign up", new_user_registration_path %></ul>
<% end %>
<ul><a href="#">faq</a></ul>
<ul><a href="#">contact</a></ul>
<ul> | </ul>
<ul><a href="#">privacy</a></ul>
<ul><a href="#">usage terms</a></ul>
<ul><a href="#">canary</a></ul>
</span>

<div class="content">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>

   <table style="width:55%; justify-cotent: center">
    <tr>
    <td><%= f.label :email %></td>
    <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
    </tr>

    <tr>
    <td><%= f.label :password %></td>
    <td><%= f.password_field :password, autocomplete: "new-password" %></td>
    </tr>

    <tr>
    <td><%= f.label :password_confirmation %></td>
    <td><%= f.password_field :password_confirmation, autocomplete: "new-password" %></td>
    </tr>
    <tr>
    <td></td>
    <td><%= f.submit "Sign up" %></td>
    </tr>
    </table>


  <%= render "users/shared/error_messages", resource: resource %>
<% end %>

</div>

<footer>
<hr>
<p>made with ruby.</p>
</footer>
<img class="sages" src="/assets/sages.png">

